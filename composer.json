{
    "name": "home/libraree-search-bundle",
    "type": "contao-bundle",
    "description": "Adds Libraree Search to Contao 4",
    "keywords":["contao","libraree", "search"],
    "license": "LGPL-3.0+",
    "authors": [
        {
            "name": "HolsteinMedia",
            "homepage": "http://www.holsteinmedia.com"
        }
    ],
    "require": {
        "php": "^5.6|^7.1",
        "contao/core-bundle": "^4.4.2",
        "home/libraree-bundle": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Home\\LibrareeSearchBundle\\": ""
        },
        "classmap": [
            "Resources/contao/"
        ],
        "exclude-from-classmap": [
            "Resources/contao/config/",
            "Resources/contao/templates/"
        ]
    },
    "extra": {
       "contao-manager-plugin": "Home\\LibrareeSearchBundle\\ContaoManager\\Plugin"
    }
}
